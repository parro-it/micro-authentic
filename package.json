{
  "name": "micro-authentic",
  "version": "1.0.0",
  "description": "Adaptation of authentic-server that work with micro.",
  "repository": "parro-it/micro-authentic",
  "scripts": {
    "test": "ava --serial && eslint ."
  },
  "keywords": [
    "micro",
    "authentication",
    "jwt"
  ],
  "author": "Andrea Parodi",
  "license": "MIT",
  "dependencies": {
    "body": "^5.1.0",
    "corsify": "^2.1.0",
    "http-hash-router": "^1.1.0",
    "ix-level-userdb": "^1.0.1",
    "jsonwebtoken": "^5.4.1",
    "level": "^1.3.0",
    "micro": "^2.0.0"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-eslint": "^5.0.0",
    "babel-plugin-module-alias": "^1.2.0",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.6.5",
    "babel-runtime": "^6.6.1",
    "bl": "^1.0.0",
    "eslint": "^2.3.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "servertest": "^1.2.1",
    "tape": "^4.2.2"
  },
  "babel": {
    "presets": [
      "babel-preset-es2015"
    ],
    "plugins": [
      "syntax-async-functions",
      "transform-async-to-generator"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard"
    ],
    "plugins": [
      "promise"
    ],
    "env": {
      "es6": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 6
    }
  }
}
